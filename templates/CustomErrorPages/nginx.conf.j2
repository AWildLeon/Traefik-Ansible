server {
    access_log /dev/null;
    error_log /dev/null;
    listen       80;
    server_name  localhost;

    {% for page in CustomErrorPages.ErrorPages %}
    error_page  {{ page.status }} {{ page.query }};
    {% endfor %}

    location / {
        root  /usr/share/nginx/error-pages;
        internal;
    }
}
